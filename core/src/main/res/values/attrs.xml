<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="radius" format="integer" />
        <attr name="radiusTopLeft" format="integer" />
        <attr name="radiusTopRight" format="integer" />
        <attr name="radiusBottomLeft" format="integer" />
        <attr name="radiusBottomRight" format="integer" />
        <attr name="fit" format="boolean" />
        <attr name="adjust" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircularColorPickerView">
        <attr name="showSaturation" format="boolean" />
        <attr name="showLuminosity" format="boolean" />
        <attr name="showAlpha" format="boolean" />
        <attr name="initialColor" format="color" />
    </declare-styleable>

    <declare-styleable name="CollapsibleLayout">
        <attr name="title" format="string" />
        <attr name="collapsed" format="boolean" />
        <attr name="collapseSpeed" format="integer" />
        <attr name="headerTextColor" format="color" />
        <attr name="headerBackgroundColor" format="color" />
    </declare-styleable>
</resources>